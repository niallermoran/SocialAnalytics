{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "connections_sql_name": {
            "defaultValue": "SQL",
            "type": "String"
        },
        "connections_sql_server": {
            "defaultValue": "sqlserver",
            "type": "String"
        },
        "connections_sql_dbname": {
            "defaultValue": "dbname",
            "type": "String"
        },
        "connections_sql_user": {
            "defaultValue": "user",
            "type": "String"
        },
        "connections_sql_password": {
            "defaultValue": "password",
            "type": "String"
        },
        "connections_twitter_name": {
            "defaultValue": "TWITTER",
            "type": "String"
        },
        "connections_cognitiveservicestextanalytics_name": {
            "defaultValue": "",
            "type": "String"
        },
        "connections_cognitiveservicestextanalytics_key": {
            "defaultValue": "",
            "type": "String"
        },
        "connections_cognitiveservicestextanalytics_url": {
            "defaultValue": "",
            "type": "String"
        }
    },
    "variables": {},
    "resources": [
        {
            "type": "Microsoft.Web/connections",
            "apiVersion": "2016-06-01",
            "name": "[parameters('connections_cognitiveservicestextanalytics_name')]",
            "location": "northeurope",
            "properties": {
                "displayName": "[parameters('connections_cognitiveservicestextanalytics_name')]",
                "parameterValues": {
                    "apiKey": "[parameters('connections_cognitiveservicestextanalytics_key')]"
                },
                "api": {
                    "id": "[concat(subscription().id, '/providers/Microsoft.Web/locations/northeurope/managedApis/cognitiveservicestextanalytics')]"
                }
            }
        },
        {
            "type": "Microsoft.Web/connections",
            "apiVersion": "2016-06-01",
            "name": "[parameters('connections_sql_name')]",
            "location": "northeurope",
            "properties": {
                "displayName": "[parameters('connections_sql_name')]",
                "parameterValues": {
                    "server": "[parameters('connections_sql_server')]",
                    "database": "[parameters('connections_sql_dbname')]",
                    "authType": "SQL",
                    "username": "[parameters('connections_sql_user')]",
                    "password": "[parameters('connections_sql_password')]"
                  },
                "customParameterValues": {},
                "api": {
                    "id": "[concat(subscription().id, '/providers/Microsoft.Web/locations/northeurope/managedApis/sql')]"
                }
            }
        },
        {
            "type": "Microsoft.Web/connections",
            "apiVersion": "2016-06-01",
            "name": "[parameters('connections_twitter_name')]",
            "location": "northeurope",
            "properties": {
                "displayName": "[parameters('connections_twitter_name')]",
                "customParameterValues": {},
                "api": {
                    "id": "[concat(subscription().id, '/providers/Microsoft.Web/locations/northeurope/managedApis/twitter')]"
                }
            }
        }
    ]
}